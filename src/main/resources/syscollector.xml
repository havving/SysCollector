<?xml version="1.0" encoding="UTF-8"?>

<configs host="192.168.0.22">
    <system-collect cpuEnable="false" memoryEnable="false" diskEnable="false" networkEnable="false"
                    engineEnable="false">
        <dirNameList> <!-- Windows delimiter=\, Unix delimiter=/ -->
            <dirname>c:\</dirname>
            <dirname>d:\</dirname>
        </dirNameList>

        <ipAddressNameList>
            <ipAddressName>192.168.10.1</ipAddressName>
        </ipAddressNameList>

        <engine destination-port="9300" clusterName="clusterName" masterIp="192.168.10.1" hotThreadCheckEnable="true"/>
    </system-collect>

    <proceess-collect>
        <!-- threshold value type: CPU - %, MEMORY - MB -->
        <process name="java"/>

    </proceess-collect>

    <es-collect masterIp="localhost" desetination-port="9200"/>

<!--    <store class="com.havving.system.domain.xml.EsStore" destination-port="9200" masterIp="localhost" indexPrefix="info"/>-->

</configs>
